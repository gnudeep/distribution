{
    "observabilityAPIURL": "http://wso2sp-observability-api/api",
    "idp": {
        "idpURL": "https://idp.cellery-system",
        "callBackURL": "http://cellery-dashboard"
    },
    "percentageRangeMinValue": {
        "errorThreshold": 0.5,
        "warningThreshold": 0.7
    },
    "templates": {
        "kubernetesMetricsLinks": {
            "pod": "http://cellery-k8s-metrics/d/cellery-k8s-pod-metrics-dashboard/pod-metrics?var-pod=${pod}&from=${fromTime}&to=${toTime}",
            "node": "http://cellery-k8s-metrics/d/cellery-k8s-node-metrics-dashboard/node-metrics?var-node=${node}&from=${fromTime}&to=${toTime}"
        }
    }
}
